# GPIO Pin Configuration for EM34A Robot Integration
BUTTON_PIN=17
BUSY_PIN=24
TRIGGER_T1_PIN=23
RESET_PIN=25
TRIGGER_VIA_PI=0   # 0 = EM34A button drives T1; Pi only watches BUSY. Set 1 if you want Pi to "press" T1.

# Printer Configuration (from print.topos)
PRINTER_DEVICE=Y812BT
PRINT_METHOD=CUPS_DIRECT

